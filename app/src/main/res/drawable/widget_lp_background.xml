<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 검은색 원형 배경 -->
    <item>
        <shape android:shape="oval">
            <solid android:color="@android:color/black"/>
        </shape>
    </item>

    <!-- 중앙의 하얀 구멍 -->
    <item
        android:left="30dp"
        android:right="30dp"
        android:top="30dp"
        android:bottom="30dp">
        <shape android:shape="oval">
            <solid android:color="@android:color/white"/>
        </shape>
    </item>
</layer-list>
